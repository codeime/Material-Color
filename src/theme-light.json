            "settings": {
                "foreground": "#90A4AE"
            }
        },
        {
            "name": "Constant keywords",
            "scope": "constant.keyword",
            "settings": {
                "foreground": "#39ADB5",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Entity name",
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#6182B8"
            }
        },
        {
            "name": "Global settings",
            "settings": {
